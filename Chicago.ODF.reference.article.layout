<text:p text:style-name="Bibliography">
<text:bookmark text:name="\bibtexkey"/>
<text:bookmark-start text:name="cite-\bibtexkey"/>

\begin{author}
\format[FormatChars,AuthorLastFirstCommas]{\author}. \end{author}
\begin{year}\format[FormatChars]{\year}. \end{year}
\format[FormatChars]{\title}. <text:span text:style-name="Emphasis">\format[FormatChars]{\journal}</text:span>
\begin{volume} \volume\end{volume}
\begin{number} (\number)\end{number}
\begin{month}, \format[FormatChars]{\month}, \end{month}
\begin{pages}: \pages\end{pages}. 

<text:bookmark-end text:name="cite-\bibtexkey"/>
</text:p>